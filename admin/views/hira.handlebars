<script data="{{hiraResult}}" >
  let data = document.currentScript.getAttribute('data');
  console.log(data)
        let wb = XLSX.utils.book_new();
        let currTimeStamp = new Date();
let dd = String(currTimeStamp.getDate()).padStart(2, '0');
let mm = String(currTimeStamp.getMonth() + 1).padStart(2, '0'); 
let yyyy = currTimeStamp.getFullYear();

currTimeStamp = dd + '-' + mm + '-' + yyyy;
        wb.Props = {
                Title: "Hira Export",
                Subject: "Hira Export",
                Author: "Vitomilix KirEx",
                CreatedDate: currTimeStamp
        };
        
        wb.SheetNames.push("Hira Export");
        var ws_data = [["some role", 45.5], ["another role", 36.4], ["role three", 9.1], ["role four", 9.1]];
        var ws = XLSX.utils.aoa_to_sheet(ws_data);
        wb.Sheets["Hira Export"] = ws;
        var wbout = XLSX.write(wb, {bookType:'xlsx',  type: 'binary'});
        function s2ab(s) {
  
                var buf = new ArrayBuffer(s.length);
                var view = new Uint8Array(buf);
                for (var i=0; i<s.length; i++) view[i] = s.charCodeAt(i) & 0xFF;
                return buf;
                
        }
        $("#button-a").click(function(){
                saveAs(new Blob([s2ab(wbout)],{type:"application/octet-stream"}), 'hira-export-' + currTimeStamp + '.xlsx');
        });
  
</script>

